			$(document).ready(function(){
			    current_page = "1";
			    $("#row4").hide();
				$("#clickme").click(function(){
					$("#portfolio").hide();
					$("#portfolio doubleup").show();
				});
				$("#page1btn").click(function(){
					current_page = "1";
					$("#row4").hide();
					$("#portfolio").show();
					$("#portfolio doubleup").hide();
				});
				$("#page2btn").click(function(){
					current_page = "2";
					$("#row4").show();
					$("#portfolio").hide();
					$("#portfolio doubleup").show();
				});
				$("#Prevbtn").click(function(){
					if (current_page == 1) {
						current_page = "2";
						$("#row4").show();
						$("#portfolio").hide();
						$("#portfolio doubleup").show();
					}
					else if (current_page == 2) {
						current_page = "1";
						$("#row4").hide();
						$("#portfolio").show();
						$("#portfolio doubleup").hide();
					}
				});		
				$("#Nextbtn").click(function(){
					if (current_page == 1) {
						current_page = "2";
						$("#row4").show();
						$("#portfolio").hide();
						$("#portfolio doubleup").show();
					}
					else if (current_page == 2) {
						current_page = "1";
						$("#row4").hide();
						$("#portfolio").show();
						$("#portfolio doubleup").hide();
					}
				});						
			});
		</script>
